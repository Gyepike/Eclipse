<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
   
    <context:annotation-config/>
    <bean id="humanObject" class="com.anti.autowired.Heart">
           <property name="numberOfPitvar" value="11"></property>
            <property name="animal" value="human"></property>
    </bean> 
   
    <bean id="horseObject" class="com.anti.autowired.Heart">
           <property name="numberOfPitvar" value="2"></property>
           <property name="animal" value="Horse"></property>
    </bean> 
     
    <bean id="headObjectValue"  class="com.anti.autowired.Head">
           <property name="animalHead" value="human"></property>
    </bean> 
    <bean id="headObjectValue2"  class="com.anti.autowired.Head">
           <property name="animalHead" value="horse"></property>
    </bean> 
   
     <bean id="legObject" class="com.anti.autowired.Leg">
          
    </bean> 
   
    
    <bean id="human" class="com.anti.autowired.Human" > 
    </bean>
</beans>